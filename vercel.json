{
  "version": 2,
  "installCommand": "yarn install --peer",
  "routes": [
    {
      "src": "/api/express",
      "dest": "/api/express.js"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**"]
      }
    }
  ]
}
